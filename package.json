{
  "name": "vk-bot.js",
  "version": "1.0.0",
  "description": "Удобная библиотека для создания ботов ВКонтакте с веб-интерфейсом для отладки",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "web/",
    "examples/",
    "README.md"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=production node lib/index.js",
    "dev": "cross-env NODE_ENV=development nodemon lib/index.js",
    "test": "jest",
    "build": "webpack --mode production",
    "dev:web": "webpack serve --mode development",
    "example:echo": "cross-env NODE_ENV=development node examples/echo-bot.js",
    "example:echo:prod": "cross-env NODE_ENV=production node examples/echo-bot.js",
    "debug": "cross-env NODE_ENV=development node web/server.js"
  },
  "keywords": [
    "vk",
    "bot",
    "vkontakte",
    "api",
    "debug",
    "web-interface"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "core-js": "^3.41.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.11.0",
    "socket.io": "^4.7.2",
    "vk-io": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
